    "description": "RoutingMiddleware which implements the Psr MiddlewareInterface",
    "type": "library",
    "license": "MIT",
    "require": {
        "psr/http-server-middleware": "^1.0",
        "symfony/routing": "^5.2",
        "symfony/psr-http-message-bridge": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-diactoros": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "PJ\\Routing\\": "src/",
            "PJ\\Routing\\Tests\\": "tests/"
